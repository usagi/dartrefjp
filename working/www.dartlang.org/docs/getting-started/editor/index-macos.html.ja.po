msgid "<li> <a href=\"index-win.html\">Windows</a> </li>"
msgstr ""

#. type: Plain text
#: index-macos.html:149
msgid "<li> <a href=\"index-linux.html\">Linux</a> </li>"
msgstr ""

#. type: Plain text
#: index-macos.html:153
msgid "</ul> </aside> </section>"
msgstr ""

